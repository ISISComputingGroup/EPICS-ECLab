## @file
## EPICS db file

# % macro, P, device prefix
# % macro, Q, device sub-prefix
# % macro, CHAN, channel number


record(stringin, "$(P)$(Q):VERSION")
{
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(PORT),0,0)VERSION")
    field(PINI, "YES")
}

