record(stringin, "$(P)ECLABVER")
{
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(PORT),0,0)VERSION")
    field(PINI, "YES")
}

record(ai, "$(P)ECLABTEST")
{
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT),0,0)PAR1")
    field(SCAN, "I/O Intr")
}

