record(stringin, "$(P)ECLAB:VER")
{
    field(DTYP, "asynOctetRead")
    field(INP, "@asyn($(PORT),0,0)VERSION")
    field(PINI, "YES")
}

record(longin, "$(P)ECLAB:DEVCODE")
{
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT),0,0)DEVCODE")
    field(PINI, "YES")
}

record(longin, "$(P)ECLAB:FIRMWAREVERS")
{
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT),0,0)FIRMWAREVERS")
    field(PINI, "YES")
}

record(longin, "$(P)ECLAB:NUMCHANNELS")
{
    field(DTYP, "asynInt32")
    field(INP, "@asyn($(PORT),0,0)NUMCHANNELS")
    field(SCAN, "I/O Intr")
}

record(stringout, "$(P)ECLAB:LOADTECH:SP")
{
    field(DTYP, "asynOctetWrite")
    field(OUT, "@asyn($(PORT),0,0)LOADTECH")
}
