# LABEL is parameter name in ECLab, PARAM is name withing ASYN/EPICS DB
pattern { P, CHAN, PORT, TECH, PARAM, LABEL, DESC, EGU, NELM }
{ "\$(P)", "\$(CHAN)", "\$(PORT)", "CA", "VSTEP", "Voltage_step", "Voltage step", "V", 100 }
{ "\$(P)", "\$(CHAN)", "\$(PORT)", "CA", "DURSTEP", "Duration_step", "Duration step", "s", 100 }
