record(ai, "$(P)ECLAB:C$(CHAN):$(TECH):$(PARAM)")
{
    field(DESC, "$(DESC)")
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT),$(CHAN),0)$(TECH)_$(PARAM)")
    field(SCAN, "I/O Intr")
	field(PREC, 3)
	field(EGU, "$(EGU)")
	field(PINI, 1)
}

