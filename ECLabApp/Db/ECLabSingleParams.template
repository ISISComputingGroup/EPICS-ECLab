{
    field(DESC, "$(DESC)")
    field(DTYP, "asynFloat64")
    field(OUT, "@asyn($(PORT),$(CHAN),0)$(TECH)_$(PARAM)")
	field(PREC, 3)
	field(EGU, "$(EGU)")
	field(PINI, 1)}
